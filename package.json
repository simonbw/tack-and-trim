{
  "name": "tack-and-trim",
  "version": "0.1.0",
  "description": "Tack and Trim - A game by Simon",
  "scripts": {
    "start": "NODE_ENV=development rm -rf .parcel-cache && concurrently -n assets,server \"npm run watch-manifest\" \"npm run dev-server\"",
    "dev-server": "parcel --no-hmr src/index.html",
    "watch-manifest": "tsx ./bin/generate-asset-types.ts -d ./resources watch",
    "build": "parcel build src/index.html",
    "tsc": "tsc --noEmit",
    "tsc-watch": "tsc --noEmit --watch",
    "prettier": "prettier --write src",
    "generate-manifest": "tsx ./bin/generate-asset-types.ts -d ./resources"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonbw/tack-and-trim.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "homepage": "https://github.com/simonbw/tack-and-trim#readme",
  "dependencies": {
    "@parcel/macros": "^2.12.0",
    "@parcel/resolver-glob": "^2.15.4",
    "@types/node": "^24.0.4",
    "@types/p2": "^0.7.45",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "glob": "^11.0.3",
    "p2": "^0.7.1",
    "parcel": "^2.15.4",
    "pixi-filters": "^6.1.3",
    "pixi.js": "^8.10.2",
    "prettier": "^3.6.0",
    "process": "^0.11.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "simplex-noise": "^4.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "yargs": "^18.0.0"
  }
}
